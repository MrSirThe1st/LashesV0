      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/lashes.png"
    },
    "extra": {
      "eas": {
        "projectId": "0104dcd0-d8c1-4f9b-983b-acf924f952b2"
